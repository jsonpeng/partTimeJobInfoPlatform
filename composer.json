{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "dingo/api": "2.0.0-alpha1",
        "doctrine/dbal": "2.5.x-dev",
        "fenos/notifynder": "dev-master",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "infyomlabs/adminlte-templates": "5.5.x-dev",
        "infyomlabs/laravel-generator": "5.5.x-dev",
        "intervention/image": "^2.4",
        "jaeger/querylist": "dev-master",
        "jaeger/querylist-phantomjs": "dev-master",
        "laracasts/flash": "~3.0.2",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5.0",
        "maatwebsite/excel": "~2.1.0",
        "mpociot/laravel-apidoc-generator": "dev-master",
        "overtrue/easy-sms": "^1.0",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-pinyin": "~3.0",
        "overtrue/laravel-shopping-cart": "~2.0",
        "overtrue/laravel-wechat": "~4.0",
        "overtrue/pinyin": "~3.0",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "spatie/laravel-backup": "5.6.4",
        "tymon/jwt-auth": "1.0.*",
        "zgldh/qiniu-laravel-storage": "^0.9.0",
        "zircote/swagger-php": "dev-master",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helper/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
